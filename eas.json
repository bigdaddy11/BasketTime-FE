{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "android": {
        "env": {
          "EXPO_PUBLIC_ENV": "development"
        }
      }
    },
    "preview": {
      "distribution": "apk",
      "channel": "preview",
      "android": {
        "env": {
          "EXPO_PUBLIC_ENV": "preview"
        }
      }
    },
    "production": {
      "distribution": "store",
      "channel": "production",
      "android": {
        "env": {
          "EXPO_PUBLIC_ENV": "production"
        }
      }
    }
  }
}